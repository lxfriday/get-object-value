{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "amd": true
  },
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/standard"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-param-reassign": 0
  }
}
